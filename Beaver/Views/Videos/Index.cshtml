@model IEnumerable<Beaver.Models.Video>
<h2>Tickets</h2>
<p>
    @Html.ActionLink("Create Ticket", "New")
</p>
<ul>
    @foreach (var video in Model)
    {
        <li>
            @video.Issue (@video.Issue)
            @Html.ActionLink("Edit", "Edit", new { id = video.Id }) |
            @Html.ActionLink("Close", "Delete", new { id = video.Id })
        </li>
    }
</ul>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}




